<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <LangVersion>8.0</LangVersion>
	  <AnalysisLevel>none</AnalysisLevel>
	  <EnableNETAnalyzers>False</EnableNETAnalyzers>
	  <GenerateDocumentationFile>True</GenerateDocumentationFile>
	  <PackageId>$(Authors).$(AssemblyName)</PackageId>
	  <Title>TicTacToeGame</Title>
	  <Authors>Kaszub09</Authors>
	  <Description>Library for playing TicTacToe game, both classic and multidimensional variants.</Description>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageTags>TicTacToe, TicTacToe3D, Multi, Dimensional</PackageTags>
	  <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>TicTacToe.AI</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>TicTacToe.Tests</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>TicTacToe.PerformanceTests</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

</Project>
